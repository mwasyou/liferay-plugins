<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.1.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_1_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.1.1+</version>
	</compatibility>
	<theme id="architekt" name="Architekt Theme">
		<settings>
			<setting configurable="false" key="show-site-name-default"  type="checkbox" value="false" />
			<setting configurable="true" key="display-search"  type="checkbox" value="true" />
			<setting configurable="true" key="header-text-article-id"  type="text"/>
			<setting configurable="true" key="header-text-cont-article-id"  type="text"/>
		</settings>
	</theme>
</look-and-feel>